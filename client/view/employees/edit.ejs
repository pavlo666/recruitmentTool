<section>
    <h3>Edit employee</h3>
    <form role="form" class="ng-pristine ng-valid" ng-controller="EmployeesEditController as panel">
        <fieldset>
            <div class="form-group">
                <label>Birthday</label>
                <input type="text" class="form-control" name="birthday" placeholder="Birthday" ng-model="data.employee.birthday">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Address</label>
                <input type="text" class="form-control" name="address" placeholder="Address" ng-model="data.employee.address">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Email</label>
                <input type="text" class="form-control" name="email" placeholder="Email" ng-model="data.employee.email">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Postal code</label>
                <input type="text" class="form-control" name="postal_code" placeholder="Postal code" ng-model="data.employee.postal_code">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Linkedin</label>
                <input type="text" class="form-control" name="url_linkedin" placeholder="Linkedin" ng-model="data.employee.url_linkedin">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Social network</label>
                <input type="text" class="form-control" name="url_social_network" placeholder="Social network" ng-model="data.employee.url_social_network">
            </div>
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label>Skype</label>
                <input type="text" class="form-control" name="skype" placeholder="Skype" ng-model="data.employee.skype">
            </div>
        </fieldset>
        <input type="hidden" name="cid" value="" ng-model="data._id">
        <button class="btn btn-sm btn-default" ng-click="panel.onClickEdit()">Save</button>
        <a href="#/candidate/{{data._id}}" class="btn btn-sm btn-default">Back</a>
    </form>

</section>